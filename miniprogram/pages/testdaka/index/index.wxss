   .timer--runing .topbox_btn_start, .timer--runing .topbox_btn_stop, .timer--runing .topbox_btn_ct, .timer--runing .topbox_btn_complete {
    filter: invert(0) !important;
    background: rgb(172, 211, 254) ;
    z-index: 99;
  }
  .timer--runing .topbox_btn {
    bottom: -1000rpx;
    flex: 0.5;
  }
  .bottomboxruning .bottombox_clock {
    margin: 3vh auto 0 auto;
    height: 100rpx;
